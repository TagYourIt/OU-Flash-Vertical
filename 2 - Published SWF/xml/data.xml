<?xml version="1.0" encoding="UTF-8"?>
<!-- please read the help file for more details -->
<data>
	
	<settings>
		
		<layout>
			<!--<Start> component width & height -->
			<width>100%</width>
			<height>100%</height>
			<minWidth>400</minWidth><!-- Default: 400 -->
			<minHeight>400</minHeight><!-- Default: 400 -->
			<offsetX>0</offsetX>
			<offsetY>0</offsetY>
			<!-- <End> -->
		</layout>
		
		<tooltips>
			<offsetY>3</offsetY>
			<offsetX>0</offsetX>
			<textOffsetY>6</textOffsetY>
			<textOffsetX>6</textOffsetX>
			<useUpperCase>true</useUpperCase><!-- Dedault: true -->
			<videoPrefix><![CDATA[PLAY:]]></videoPrefix>
			<audioPrefix><![CDATA[PLAY:]]></audioPrefix>
			<flashPrefix><![CDATA[VIEW:]]></flashPrefix>
			<imagePrefix><![CDATA[VIEW:]]></imagePrefix>
			<pagination><![CDATA[SHOW %FROM% TO %TO% OUT OF %TOTAL%]]></pagination>
		</tooltips>
		
		<thumbCell>
		
			<width>200</width><!-- Default: 200 -->
			<height>130</height><!-- Default: 130 -->

			<thumbnail>
				
				<maxRotation>30</maxRotation>
				<border>
					<size>4</size>
					<color>0xFFFFFF</color>
				</border>
				<innerBorder>
					<size>4</size>
					<color>0xffffff</color>
					<alpha>0.15</alpha>
				</innerBorder>
				<fileTypeIcon>
					<visible>true</visible>
					<normalAlpha>0.3</normalAlpha>
					<overAlpha>0.7</overAlpha>
				</fileTypeIcon>
				<shadow>
					<visible>true</visible>
					<color>0x000000</color>
					<alpha>0.4</alpha>
					<blur>4</blur>
					<angle>120</angle>
					<distance>0</distance>
				</shadow>
				
			</thumbnail>
			
		</thumbCell>
		
		<pagination>
			<alwaysDisplay>false</alwaysDisplay><!-- Default: false -->
			<!--
				align type:
				<align>top</align> - it will align top center
				<align>bottom</align> - it will align bottom center
				<align>left</align> - it will align left center
				<align>right</align> - it will align right center
				<align>center</align> - it will align center
				<align>topLeft</align> - it will align top left
				<align>topRight</align> - it will align top right
				<align>bottomLeft</align> - it will align bottom left
				<align>bottomRight</align> - it will align bottom right
			-->
			<align>bottom</align><!-- field case sensitive, use the above strings -->
			<offsetX>0</offsetX>
			<offsetY>0</offsetY>
			
			<thumbOffset>
				<offsetX>3</offsetX>
				<offsetY>0</offsetY>
				<holderOffsetX>5</holderOffsetX>
				<holderOffsetY>5</holderOffsetY>
			</thumbOffset>
			
		</pagination>
		
		<detailView>
			<useUpperCase>true</useUpperCase><!-- Dedault: true -->
			<hideOffLimitPicture>true</hideOffLimitPicture><!-- Default: true -->
			
			<animation>
				<bringToFrontTime>0.5</bringToFrontTime><!-- Default: 0.5 -->
				<bringToFrontType>easeoutquad</bringToFrontType>
				
				<sentToBackTime>0.5</sentToBackTime><!-- Default: 0.5 -->
				<sentToBackType>easeoutquad</sentToBackType>
			</animation>
					
			<closeBtn>
				<label><![CDATA[CLOSE]]></label>
				<normalAlpha>0.3</normalAlpha>
				<overAlpha>0.5</overAlpha>
			</closeBtn>
			
			<playBtn>
				<normalAlpha>0.3</normalAlpha>
				<overAlpha>0.7</overAlpha>
			</playBtn>
				
			<margin>25</margin><!-- Margin used for resize to fit 25 -->
			
			<video>
				<animation>
					<bringToFrontTime>0.5</bringToFrontTime>
					<bringToFrontType>easeoutquad</bringToFrontType>
				</animation>
				
				<preloaderColor>0xffffff</preloaderColor>
				<titlePrefix><![CDATA[VIDEO:]]></titlePrefix>
				<autoPlay>true</autoPlay>
				<repeat>true</repeat>
				<buffer>1</buffer>
				<initVolume>0.3</initVolume>
				<!-- fit options: fitToFill, fitToSize, fitToSizeForced, stretch & normal. -->
				<fit>fitToSizeForced</fit><!-- fitToSizeForced -->
			</video>
			
			<audio>
				<animation>
					<bringToFrontTime>2</bringToFrontTime>
					<bringToFrontType>easeoutquad</bringToFrontType>
				</animation>
				
				<preloaderColor>0xffffff</preloaderColor>
				<titlePrefix><![CDATA[AUDIO:]]></titlePrefix>
				<autoPlay>false</autoPlay>
				<repeat>false</repeat>
				<buffer>1</buffer>
				<initVolume>0.1</initVolume>
			</audio>
			
			<flash>
				<animation>
					<bringToFrontTime>2</bringToFrontTime>
					<bringToFrontType>easeoutquad</bringToFrontType>
				</animation>
				
				<titlePrefix><![CDATA[FLASH:]]></titlePrefix>
			</flash>
			
			<image>
				<animation>
					<bringToFrontTime>2</bringToFrontTime>
					<bringToFrontType>easeoutquad</bringToFrontType>
				</animation>
				
				<titlePrefix><![CDATA[IMAGE:]]></titlePrefix>
			</image>
			
		</detailView>
		
		<youtube>
		
			<player>http://www.youtube.com/apiplayer?version=3</player>
			<securityDomain>http://www.youtube.com</securityDomain>
			
			<policyFiles>
				<policy>http://gdata.youtube.com/crossdomain.xml</policy>
				<policy>http://video-stats.video.google.com/crossdomain.xml</policy>
				<policy>http://www.youtube.com/crossdomain.xml</policy>
				<policy>http://s2.youtube.com/crossdomain.xml</policy>
				<policy>http://s.ytimg.com/crossdomain.xml</policy>
				<policy>http://i.ytimg.com/crossdomain.xml</policy>
			</policyFiles>
			
			<API>
				<location>http://gdata.youtube.com/feeds/api/videos/</location>
				<version>2</version>
				<format>5</format>
			</API>
			
			<error>
				<offsetX>0</offsetX>
				<offsetY>0</offsetY>
				<padX>5</padX>
				<padY>4</padY>
				<notFound><![CDATA[<p>The requested video has been removed</p>]]></notFound>
				<notAllowed><![CDATA[<p>The requested video does not allow playback in the embedded players</p>]]></notAllowed>
			</error>
			
		</youtube>
		
	</settings>
	
	<content>
		
		
		
		
		
		<item>
			<type>video</type>
			<title><![CDATA[HOME]]></title>
			<thumbnail></thumbnail>
			<detailView>
				<width>800</width>
				<height>450</height>
				<source>youtube</source>
				<file>J7gCUvf38jE</file>
				<quality>hd720</quality>
			</detailView>
		</item>
		
		
		<item>
			<type>video</type>
			<title><![CDATA[GOOGLE I/O 2010]]></title>
			<thumbnail></thumbnail>
			<detailView>
				<width>800</width>
				<height>450</height>
				<source>youtube</source>
				<file>pAqCXUbCz8I</file>
				<quality>hd720</quality>
			</detailView>
		</item>
		
		
		
		
		
		
		
		
		
		
		
	</content>
	
</data>
